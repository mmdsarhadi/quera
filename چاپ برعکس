import java.util.Scanner;

public class baraks {

	public static void main(String[] args) {
		Scanner mmd = new Scanner(System.in);
		int[] m = new int[1000];
		int x  = 0;
		
		for (int i =0 ; i <100;i++)
			
		{
			m[i] = mmd.nextInt();
			if(m[i] == 0)
				
			{
				x = i;
				break;
			}
		}
for (int i =x-1 ; i >=0 ;i--)
			
		}
	System.out.println(m[i]);
		}
	}}
